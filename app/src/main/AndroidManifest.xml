<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="io.github.seibelsabrina.sheltermefb">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Shelter Me"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LoginScreenActivity" />
        <activity android:name=".RegisterScreenActivity" />
        <activity android:name=".MainAdminActivity" />
        <activity android:name=".AddShelterActivity" />
        <activity android:name=".MainUserActivity" />
        <activity android:name=".ShelterListActivity" />
        <activity android:name=".ShelterDetailViewActivity" />
        <activity android:name=".SearchActivity" />
        <activity android:name=".BookRoomActivity"></activity>
    </application>

</manifest>